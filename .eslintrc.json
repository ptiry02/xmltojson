{
	"env": {
		"node": true,
		"browser": false,
		"es2021": true
	},
	"extends": ["eslint:recommended", "plugin:prettier/recommended"],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["prettier"],
	"rules": {
		"prettier/prettier": "error",
		"indent": ["error", "tab"],
		"linebreak-style": ["error", "unix"],
		"quotes": ["error", "single"],
		"semi": ["error", "never"]
	}
}
